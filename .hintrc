{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "button-type": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scrollbar-width"
        ]
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 109",
    "not ios_saf <= 18.4",
    "not safari <= 18.4",
    "not samsung <= 27"
  ]
}